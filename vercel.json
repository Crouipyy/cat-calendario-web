{
  "version": 2,
  "builds": [
    {
      "src": "api/**/*.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "/api/$1"
    },
    {
      "src": "/nui/(.*)",
      "dest": "/nui/$1"
    },
    {
      "src": "/web",
      "dest": "/nui/index.html"
    },
    {
      "src": "/",
      "dest": "/nui/index.html"
    }
  ]
}

