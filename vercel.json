{
  "rewrites": [
    {
      "source": "/api/(.*)",
      "destination": "/api/$1"
    },
    {
      "source": "/web",
      "destination": "/nui/index.html"
    },
    {
      "source": "/",
      "destination": "/nui/index.html"
    }
  ],
  "headers": [
    {
      "source": "/nui/(.*)",
      "headers": [
        {
          "key": "Access-Control-Allow-Origin",
          "value": "*"
        }
      ]
    }
  ]
}

